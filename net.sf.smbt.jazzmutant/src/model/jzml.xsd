      <attribute name="hold" type="string"/>
      <attribute name="holdX" type="string"/>
      <attribute name="holdY" type="string"/>
  </complexType>

  <complexType name="PARAM">
      <attribute name="name" type="string"/>
      <attribute name="value" type="string"/>
      <attribute name="send" type="string"/>
      <attribute name="osc_target" type="string"/>
      <attribute name="osc_trigger" type="string"/>
