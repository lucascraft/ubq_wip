<SCRIPT name="scaleQuant(note)" script="decl notes = cNoteHand.swScale.x;
decl i = 0;
decl j = 0;
decl n = 0;

note = round(note);
n = round(note-cNoteHand.pdRootNote.number) % 12;

for (i=0; i<12; i++) {
	j = round((n+i) % 12);
	if (notes[j]) 
	{ 
		note = note + i; 
		break; 
	}
}
return note;" trigger_script="" trigger_type="4" trigger="1" osc_message="/scaleQuant" midi_message="0x90,0x90,0,0" midi_target="-2" flag="0"/>
<SCRIPT name="SpdTransform(id, source)" script="decl Min,Max,Index,i,out;
Index = id * 2;
Min = 1/Spd_Interpol[Index];
Max	= Spd_Interpol[Index];
for(i=0; i < Points;i++){	
	
	out[i] = range(source[i],Min,Max);
}
return out;" trigger_script="" trigger_type="4" trigger="1" osc_message="/SpdTransform" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
