rst = 1;
cControls.paControls.pos = -1;" trigger_script="" trigger_type="4" trigger="1" osc_message="/Arrays/cNumericPad/pNumericPads/sendValue" midi_message="0x90,0x90,0,0" midi_target="-2" flag="0"/>
