<SCRIPT name="SendNote()" script="if(x == 0) return;
decl i;
for(i = 0; i < 32;i++){
	
	if(x && paNote.x[i] && !Nstatus[i])
	{
		noteout(target, NotePitch[i], Velocity[i]*paNote.x[i], MidiCH);
		Nstatus[i] = 1;
		break;
	}
	
	if(!paNote.x[i] && Nstatus[i] && !NoteStatus)
	{
		noteout(target, NotePitch[i], 0 , MidiCH);
		Nstatus[i] = 0;
	}
}
" trigger_script="paNote.x" trigger_type="0" trigger="1" osc_message="/cNoteHand/cbMidiMonitor/SendNote" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
