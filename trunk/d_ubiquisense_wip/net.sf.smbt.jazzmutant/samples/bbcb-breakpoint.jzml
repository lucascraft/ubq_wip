<SCRIPT name="onPress()" script="if (firstof(x) == 0) setValue(1);
if (firstof(x) == 1) setValue(2);
if (firstof(x) == 2) setValue(3);
if (firstof(x) == 3) setValue(4);
if (firstof(x) == 4) setValue(5);
if (firstof(x) == 5) setValue(6);
if (firstof(x) == 6) setValue(7);
if (firstof(x) == 7) setValue(8);
if (firstof(x) == 8) setValue(9);
if (firstof(x) == 10) setValue(0);

if (firstof(x) == 12) { decValue(); return; }
if (firstof(x) == 13) { midValue(); return; }
if (firstof(x) == 14) { incValue(); return; }

if (firstof(x) == 15) { resetValue(); return; }
if (firstof(x) == 17) { sendValue(); return; }

