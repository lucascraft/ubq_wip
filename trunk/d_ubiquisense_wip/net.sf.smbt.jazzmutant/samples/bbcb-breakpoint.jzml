<SCRIPT name="motion()" script="decl pos = range(VVal,0.5,1) * VVal;
decl limit = range(pos,0.01,0.99);
decl i, items = 32;
if(!cClk.clock.x || meVekMode.selection == 0)return;
decl Dur = (cClk.tempo.bpm/60)*Rate.val;
decl AX = Amp.x*0.5;
decl AY = Amp.y*0.5;

if(meVekMode.selection == 1)//XY Rate Linked
{
	for(i=0; i < items;i++)
	{
		Vektor.x[i] = sin(cClk.clock.runTime*Dur*Phase[i]*AxisRate.x[0])*limit[i]*AX+0.5;
		Vektor.y[i] = cos(cClk.clock.runTime*Dur*Phase[i]*AxisRate.x[0])*limit[i]*AY+0.5;
	}
	return;
}
if(meVekMode.selection == 2)
{
	Vektor.x = sin(cClk.clock.runTime*Dur*Phase*AxisRate.x[0])*limit*AX+0.5;
	Vektor.y = cos(cClk.clock.runTime*Dur*Phase*AxisRate.x[1])*limit*AY+0.5;
	return;
}
if(meVekMode.selection == 3)////Limit Bounce
{
	Vektor.x = sin(cClk.clock.runTime*Dur*Phase*AxisRate.x[0])*limit*Amp.x+0.5;
	Vektor.y = 1-(cos(cClk.clock.runTime*Dur*Phase*AxisRate.x[1])*limit*Amp.y+0.5);
	return;
}
if(meVekMode.selection == 4)//Petalos
{
	Vektor.x = cos(cClk.clock.runTime*Dur*Phase*AxisRate.x[1])*pow(cos(2*cClk.clock.runTime*Dur*Phase*AxisRate.x[0]),2)*limit*AX+0.5;
	Vektor.y = sin(cClk.clock.runTime*Dur*Phase*AxisRate.x[1])*pow(cos(2*cClk.clock.runTime*Dur*Phase*AxisRate.x[0]),2)*limit*AY+0.5;
	return;
}
if(meVekMode.selection == 5)//Xonly
{
	Vektor.x = ((cClk.clock.runTime*Dur*Phase*AxisRate.x[0])*0.1) % 1;
	Vektor.y = Align;
	return;
}
if(meVekMode.selection == 6)//Reverse Xonly
{
	Vektor.x = 1-((cClk.clock.runTime*Dur*Phase*AxisRate.x[0])*0.1) % 1;
	Vektor.y = Align;
	return;
}
if(meVekMode.selection == 7)
{
	Vektor.x = sin(cClk.clock.runTime*Dur*Phase*AxisRate.x[0])*AX+0.5;
	Vektor.y = Align;
	return;
	
}
if(meVekMode.selection == 8)
{
	Vektor.x = sin(cClk.clock.runTime*Dur*Phase*AxisRate.x[0])*limit*AX+0.5;
	Vektor.y = cos(cClk.clock.runTime*Dur*Phase*AxisRate.x[1]*0.1)*limit*AY+0.5;
	return;
}
" trigger_script="cClk.clock.runTime" trigger_type="0" trigger="1" osc_message="/motion" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="PhTransform(Phase, Wrap)" script="decl ShapeX,ShapeY,NodesN,ShapeModA,ShapeModB,ShapeSum,MulAdd,out;

NodesN = Points-1;
ShapeX = (pow(Wrap * 2 + (-1),3) * 0.5) * (-1);
ShapeY = (pow(Phase * 2 + (-1),3)) * 0.5;
decl i;
for(i = 0;i < Points;i++){
	
	ShapeModA[i] = NodesN * Align[i] * ShapeY;
	ShapeModB[i] = NodesN * ShapeY * (-0.5);
	ShapeSum[i] = ShapeModA[i] + ShapeModB[i] + ShapeX;
	MulAdd[i] = ShapeSum[i] * 2 + 0.5;
	out[i] = abs(MulAdd[i]) % 1;
}
return out;

" trigger_script="" trigger_type="4" trigger="1" osc_message="/PhTransform" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
