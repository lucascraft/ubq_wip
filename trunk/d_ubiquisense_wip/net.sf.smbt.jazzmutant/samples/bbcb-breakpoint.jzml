<SCRIPT name="Invert(Array)" script="decl i;
decl len = sizeof(Array);
for(i = 0; i < len; i++){
	
	temp[i] = round(Array*24);
	Array[i] = 1 - (temp[i]* (1/24));
}
return Array;" trigger_script="" trigger_type="4" trigger="1" osc_message="/Invert" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="Reverse(Array)" script="temp = round(Array*24);
decl i;
decl len = sizeof(Array);
for(i = 0; i < len; i++){
	
	Array[i] = temp[(len-1)-i] * 1/24;
}
return Array;" trigger_script="" trigger_type="4" trigger="1" osc_message="/Reverse" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="ValDn(Array)" script="//VAL ARRAY DOWN (WITH MOD)
temp = round(Array*24);
decl StpSize = 1;
decl i;
decl len = sizeof(Array);
for(i = 0;i < 32;i++){
	
	temp[i] -= StpSize;
	temp[i] %= 96;
	Array[i] = temp[i] * 1/24;
}
return Array;" trigger_script="" trigger_type="4" trigger="1" osc_message="/ValDn" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="ValUp(Array)" script="//VAL ARRAY UP (WITH MOD)
temp = round(Array*24);
decl StpSize = 1;
decl i;
decl len = sizeof(Array);
for(i = 0;i < len;i++){
	
	temp[i] += StpSize;
	temp[i] %= 25;
	Array[i] = temp[i] * 1/24;
}
return Array;" trigger_script="" trigger_type="4" trigger="1" osc_message="/ValUp" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
