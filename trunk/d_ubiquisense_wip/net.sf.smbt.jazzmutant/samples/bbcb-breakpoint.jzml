<SCRIPT name="process()" script="decl notes = msPitch.nProcess;
decl i = 0;
decl nbr = Points;

for (i=0; i<nbr; i++) {

	if (x[i] > range(raX.x[1], 0.02, 0.98) && !NoteStatus[i]) {
		NotePitch[i] = notes[i]; //+ baseNote;
		NotePitch[i] = clamp(NotePitch[i],0,120);
		NotePitch[i] = scaleQuant(NotePitch[i]);
		Velocity[i] = round(range(Arrays.TableC.x[i],rngVelo.x[0],rngVelo.x[1])*127);
		NoteStatus[i] = 1;
		break;
	}

	if (x[i] < range(raX.x[0], 0.02, 0.98) && NoteStatus[i]) {
		NoteStatus[i] = 0;
		break;
	}

}
setattribute(cNoteHand.paNote,'colors',RGB(NotePitch/120,0.35-(0.35*NotePitch/120),0.5-(0.5*NotePitch/120)));
setattribute(Arrays.TableB,'colors',RGB(NotePitch/120,0.35-(0.35*NotePitch/120),0.5-(0.5*NotePitch/120)));
GetNotes(NotePitch);
" trigger_script="x" trigger_type="0" trigger="1" osc_message="/Vektor/process" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
