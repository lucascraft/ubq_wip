<SCRIPT name="doNotes()" script="decl i = 0;
decl max = 32;

for (i = 0; i < max; i++) {

	if (cNoteHand.paNote.status[i] && NoteStatus[i] && ! noteOn[i]) {
		noteout(target, NotePitch[i], Velocity[i], MidiCH);
		noteOn[i] = 1;
		cNoteHand.leStatus.value = noteOn;
	}

	if (!NoteStatus[i] && noteOn[i]) {
		noteout(target, NotePitch[i], 0, MidiCH);
		noteOn[i] = 0;
		cNoteHand.leStatus.value = noteOn;
	}

}
" trigger_script="" trigger_type="4" trigger="1" osc_message="/cClk/doNotes" midi_message="0x90,0x90,0,0" midi_target="-2" flag="0"/>
