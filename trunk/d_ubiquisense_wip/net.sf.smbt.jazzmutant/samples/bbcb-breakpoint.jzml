<SCRIPT name="interpolate()" script="decl pos = cbMorph.x;
decl items = 2;
if (!pos) return;
decl dist;
decl i;
for (i =0 ; i < items; i++) {
	
	decl a = (x-1[i])*(x-1[i]);
	if (a<0.00001f) a = 0.00001f;
	dist[i] = a;
}
decl weight = 1/dist;
weight /= sumof(weight);
decl val = stretch(0,8);
decl j;

for (i = 0; i < 8; i++) {
	
	for (j = 0; j < items; j++) {
		
		val[i] += presets[pos[j]*8+i]*weight[j];
	}
}
mbControls.y = val;
if(x < 0.02){Load.index = 0;return;}
if(x > 0.98){Load.index = 1;return;}" trigger_script="{x,cbMorph.x}" trigger_type="0" trigger="1" osc_message="/Arrays/cP2/Morph/interpolate" midi_message="0x90,0x90,0,0" midi_target="-2" flag="1"/>
<SCRIPT name="Set(pos)" script="if(pos == 0) {x = 0};
if(pos == 1) {x = 1};" trigger_script="" trigger_type="4" trigger="1" osc_message="/Arrays/cP2/Morph/Set" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
