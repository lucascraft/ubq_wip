	// create a stable LFO that stops when we want it to
	LFO+=0.01;

	// speed variable
	decl foo = XWaveSpeed.out*LFO;

	// width variable
	decl bar = values*XWaveWidth.out;

	// amplitude
	decl amp = 2;

	// spirals?
	if(XWaveSet.x[4]){
		decl mod = (8*PI)+(foo*1.5);
		decl a = 1/(2*mod);
		decl t = stretch({0,mod},32);
		x = (a*t*cos(t+(XWaveWidth.out*foo/3)))+0.5;
	}else if(XSinCos.x[0]){
		x = (sin((foo)+(bar))+1)/amp;
	}else{
		x = (cos((foo)+(bar))+1)/amp;
