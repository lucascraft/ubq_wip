<SCRIPT name="set_tones()" script="decl scaleout;  

if(selection==0) scaleout = {2, 4, 5, 7, 9, 11}; // Major
if(selection==1) scaleout = {2, 3, 5, 7, 8, 10}; // Natural Minor
if(selection==2) scaleout = {2, 3, 5, 7, 8, 11}; // Harmonic Minor
if(selection==3) scaleout = {2, 3, 5, 7, 9, 11}; // Melodic Minor
if(selection==4) scaleout = {2, 3, 5, 7, 9, 10}; // Dorian
if(selection==5) scaleout = {2, 4, 5, 7, 9, 10}; // Mixolydian

Length.x = (sizeof(scaleout)-4)*1/4;
Length.out = sizeof(scaleout)+1;
ScaleTones.x = scaleout/11;" trigger_script="selection" trigger_type="0" trigger="1" osc_message="/NodesNotes/ScaleTrack4/Selecta/set_tones" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
</WINDOW>
