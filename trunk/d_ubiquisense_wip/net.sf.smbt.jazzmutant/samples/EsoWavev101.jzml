<SCRIPT name="docopy()" script="if(firstof(x)==0){
	ScaleTrack1.Selecta.selection = Selecta.selection;
	ScaleTrack1.Length.x = Length.x;
	ScaleTrack1.Octave.x = Octave.x;
	ScaleTrack1.RootNote.x = RootNote.x;
	ScaleTrack1.ScaleTones.x = ScaleTones.x;

	ScaleTrack3.Selecta.selection = Selecta.selection;
	ScaleTrack3.Length.x = Length.x;
	ScaleTrack3.Octave.x = Octave.x;
	ScaleTrack3.RootNote.x = RootNote.x;
	ScaleTrack3.ScaleTones.x = ScaleTones.x;

	ScaleTrack4.Selecta.selection = Selecta.selection;
	ScaleTrack4.Length.x = Length.x;
	ScaleTrack4.Octave.x = Octave.x;
	ScaleTrack4.RootNote.x = RootNote.x;
	ScaleTrack4.ScaleTones.x = ScaleTones.x;
